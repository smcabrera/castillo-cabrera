source ~/.rvm/scripts/rvm
# making ls faster and adding color
alias l="ls --color"
alias la="ls -a --color"
alias ll="ls -l --color"
alias lla="ls -la --color"

# Defining the $EDITOR shell environment for tmuxinator
export EDITOR=vim


### Added by the Heroku Toolbelt
export PATH="/usr/local/heroku/bin:$PATH"
